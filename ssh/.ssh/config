Include ~/.colima/ssh_config

Host git@github.com:mrge-group
  AddKeysToAgent yes
  IdentityFile ~/.ssh/mrge_ed25519

Host vps
    HostName vps.joseederangojr.site
    User admin
    Port 22
    IdentityFile ~/.ssh/id_ed25519
    ForwardAgent no
    ServerAliveInterval 60

Host *
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519
